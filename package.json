{
  "name": "ts-project-tpl",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "typings": "dist/types",
  "unpkg": "dist/taco.min.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "analyze": "cross-env NODE_ENV=production ANALYZE_MODE=on webpack --mode=production",
    "build:w": "cross-env NODE_ENV=production webpack --watch --mode=production",
    "dev": "cross-env NODE_ENV=development webpack --mode=development",
    "dev:w": "cross-env NODE_ENV=development webpack --watch --mode=development",
    "all": "npm run dev && npm run build",
    "lint": "tslint -c tslint.json \"./src/**/*.ts\" --force",
    "lint:fix": "tslint -c tslint.json --fix \"./src/**/*.ts\" --force",
    "test": "jest",
    "test:cover": "jest --coverage",
    "format": "prettier --write ./src/**/**.ts",
    "pre:commit": "npm run lint:fix && npm run format && npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jest/types": "^24.9.0",
    "@types/jest": "^24.0.22",
    "cross-env": "^6.0.3",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.10"
  },
  "dependencies": {}
}